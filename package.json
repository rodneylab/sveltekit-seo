{
  "name": "sveltekit-seo",
  "version": "0.3.1",
  "scripts": {
    "dev": "svelte-kit dev --port 3000",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview --port 3000",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@fontsource/lato": "^4.5.0",
    "@fontsource/lora": "^4.5.0",
    "@fontsource/source-sans-pro": "^4.5.0",
    "@imgix/js-core": "^3.2.1",
    "@sveltejs/adapter-static": "^1.0.0-next.13",
    "@sveltejs/kit": "^1.0.0-next.137",
    "dayjs": "^1.10.6",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "mdsvex": "^0.9.8",
    "object-hash": "^2.2.0",
    "prettier": "~2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "reading-time": "^1.3.0",
    "sass": "^1.36.0",
    "svelte": "^3.40.2",
    "svelte-feather-icons": "^3.5.0",
    "svelte-preprocess": "^4.7.4"
  },
  "type": "module",
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run prettier:check"
    }
  }
}
