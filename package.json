{
  "name": "sveltekit-seo",
  "version": "0.3.2",
  "scripts": {
    "dev": "svelte-kit dev --port 3000",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview --port 3000",
    "check": "svelte-check --fail-on-hints",
    "check:watch": "svelte-check --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "lint:scss": "stylelint \"src/**/*.{css,scss,svelte}\"",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. .",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.0",
    "@fontsource/lato": "^4.5.8",
    "@fontsource/lora": "^4.5.9",
    "@fontsource/source-sans-pro": "^4.5.9",
    "@iconify/svelte": "^2.2.1",
    "@rodneylab/sveltekit-components": "^0.2.12",
    "@sveltejs/adapter-static": "^1.0.0-next.32",
    "@sveltejs/kit": "^1.0.0-next.344",
    "@types/object-hash": "^2.2.1",
    "commitlint": "^17.0.1",
    "dayjs": "^1.11.2",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.2",
    "mdsvex": "^0.10.5",
    "object-hash": "^3.0.0",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "prettier": "~2.5.1",
    "prettier-plugin-svelte": "^2.7.0",
    "reading-time": "~1.4.0",
    "sass": "^1.52.1",
    "stylelint": "^14.8.4",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-scss": "^4.2.0",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.1",
    "svelte-preprocess": "^4.10.6",
    "vanilla-lazyload": "^17.8.2",
    "vite-imagetools": "^4.0.3"
  },
  "type": "module",
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}
