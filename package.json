{
  "name": "sveltekit-seo",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev --port 3000",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview --port 3000",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "prettier:check": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@fontsource/lato": "^4.4.5",
    "@fontsource/lora": "^4.4.5",
    "@fontsource/source-sans-pro": "^4.4.5",
    "@imgix/js-core": "^3.2.1",
    "@sveltejs/adapter-netlify": "^1.0.0-next.17",
    "@sveltejs/kit": "^1.0.0-next.119",
    "dayjs": "^1.10.5",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "mdsvex": "^0.9.7",
    "prettier": "~2.3.2",
    "prettier-plugin-svelte": "^2.3.1",
    "reading-time": "^1.3.0",
    "sass": "^1.35.1",
    "svelte": "^3.38.3",
    "svelte-feather-icons": "^3.5.0",
    "svelte-preprocess": "^4.7.3"
  },
  "type": "module",
  "dependencies": {
    "husky": "^6.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run prettier:check"
    }
  }
